  <actions>
    <action id="RakuPlugin.NewScriptAction" class="edument.rakuidea.actions.NewScriptAction" text="Raku Script"
            description="Create new Raku script in current project" icon="/icons/camelia.png">
      <add-to-group group-id="NewGroup" anchor="after" relative-to-action="NewDir"/>
    </action>
    <action id="RakuPlugin.NewModuleAction" class="edument.rakuidea.actions.NewModuleAction" text="Raku Module"
            description="Create new Raku module in current project" icon="/icons/camelia.png">
      <add-to-group group-id="NewGroup" anchor="after" relative-to-action="RakuPlugin.NewScriptAction"/>
    </action>
    <action id="RakuPlugin.NewTestAction" class="edument.rakuidea.actions.NewTestAction" text="Raku Test"
            description="Create new Raku test in current project" icon="/icons/camelia.png">
      <add-to-group group-id="NewGroup" anchor="after" relative-to-action="RakuPlugin.NewModuleAction"/>
    </action>
    <action id="RakuPlugin.NewPodDocumentation" class="edument.rakuidea.actions.NewDocumentationAction" text="Raku Documentation (Pod6)"
            description="Create new Raku documentation file in current project" icon="/icons/camelia.png">
      <add-to-group group-id="NewGroup" anchor="after" relative-to-action="RakuPlugin.NewTestAction"/>
    </action>
    <action id="RakuPlugin.NewCroTemplateAction" class="edument.rakuidea.cro.actions.NewCroTemplateAction" text="Cro Template"
            description="Create new Cro template in current project" icon="/icons/cro.png">
      <add-to-group group-id="NewGroup" anchor="after" relative-to-action="RakuPlugin.NewPodDocumentation"/>
    </action>
    <action id="edument.rakuidea.repl.RakuLaunchReplAction" class="edument.rakuidea.repl.RakuLaunchReplAction" text="Launch Raku REPL"
            description="Runs the Raku REPL associated with the current project&#39;s SDK.">
      <add-to-group group-id="ToolsMenu" anchor="after" relative-to-action="ExternalToolsGroup"/>
    </action>
    <action id="RakuReplExecute" class="com.intellij.openapi.actionSystem.EmptyAction"
            text="Execute Raku Code"
            description="Execute Raku code in REPL console">
      <keyboard-shortcut first-keystroke="control ENTER" keymap="$default"/>
    </action>
    <action id="edument.rakuidea.repl.RakuReplUsingThisModuleAction" class="edument.rakuidea.repl.RakuReplUsingThisModuleAction"
            text="REPL Using This Module" description="Launch a Raku REPL with this module used">
      <add-to-group group-id="EditorPopupMenu" anchor="last"/>
    </action>
    <action id="edument.rakuidea.grammar.RakuGrammarPreviewAction" class="edument.rakuidea.grammar.RakuGrammarPreviewAction"
            text="Preview Raku Grammar"
            description="Opens a tool to preview Raku grammar parsing results">
      <add-to-group group-id="ToolsMenu" anchor="after" relative-to-action="ExternalToolsGroup"/>
    </action>
    <action id="edument.rakuidea.actions.ShowSecondarySdkSetter" class="edument.rakuidea.actions.ShowSecondarySdkSetter"
            text="Setup Raku SDK"
            description="Opens a tool to setup Raku SDK of this project, which can be used by different components">
      <add-to-group group-id="ToolsMenu" anchor="after" relative-to-action="ExternalToolsGroup"/>
    </action>
    <action id="edument.rakuidea.actions.UpdateExtensionsAction" class="edument.rakuidea.actions.UpdateExtensionsAction"
            text="Update Raku File Extensions"
            description="Renames files with obsolete Raku extensions to use official replacement ones">
      <add-to-group group-id="ToolsMenu" anchor="after" relative-to-action="ExternalToolsGroup"/>
    </action>
    <action id="RakuPlugin.ExtractPrivateMethod" class="edument.rakuidea.actions.ExtractPrivateMethodAction" text="Private Method..."
            description="Turn this selected code fragment into a private method">
      <keyboard-shortcut keymap="$default" first-keystroke="ctrl alt P"/>
      <add-to-group group-id="IntroduceActionsGroup" anchor="after" relative-to-action="ExtractMethod"/>
    </action>
    <action id="RakuPlugin.ExtractRoutine" class="edument.rakuidea.actions.ExtractRoutineAction" text="Subroutine..."
            description="Turn this selected code fragment into a subroutine">
      <keyboard-shortcut keymap="$default" first-keystroke="ctrl alt R" second-keystroke="O"/>
      <add-to-group group-id="IntroduceActionsGroup" anchor="before" relative-to-action="ExtractMethod"/>
    </action>
    <action id="RakuPlugin.ExtractRegex" class="edument.rakuidea.actions.ExtractRegexAction" text="Extract Rule..."
            description="Turn this selected code fragment into a separate regex">
      <keyboard-shortcut keymap="$default" first-keystroke="ctrl alt R" second-keystroke="E"/>
      <add-to-group group-id="IntroduceActionsGroup" anchor="before" relative-to-action="ExtractMethod"/>
    </action>
    <group id="RakuPlugin.CoverageMenu">
      <action id="RakuPlugin.HideCoverage" class="edument.rakuidea.coverage.RakuCoverageHideAction"
              text="Hide Raku Coverage Data"/>
      <add-to-group group-id="CoverageMenu"/>
      <separator/>
    </group>
    <action id="edument.rakuidea.actions.LoadProfileResultsAction" class="edument.rakuidea.actions.LoadProfileResultsAction"
            text="Show And Compare Recent Raku Profiles..." description="Select and load one of the recent Raku profiling results snapshot from this project or select two and compare">
      <add-to-group group-id="RunMenu"/>
    </action>
    <action id="edument.rakuidea.actions.LoadProfilerSQLFromDiskAction" class="edument.rakuidea.actions.LoadProfilerSQLFromDiskAction"
            text="Import Raku Profile SQL..."
            description="Imports a profiler report from an SQL data file">
      <add-to-group group-id="RunMenu" anchor="after" relative-to-action="edument.rakuidea.actions.LoadProfileResultsAction"/>
    </action>

    <action id="edument.rakuidea.actions.ExtractParentClassAction" class="edument.rakuidea.actions.ExtractParentClassAction"
            text="Extract Parent Class..." description="Extract a parent class with selected attributes and methods">
      <add-to-group group-id="IntroduceActionsGroup" anchor="after" relative-to-action="RakuPlugin.ExtractRegex"/>
    </action>
    <action id="edument.rakuidea.actions.ExtractRoleClassAction" class="edument.rakuidea.actions.ExtractRoleClassAction"
            text="Extract Role..." description="Extract a role with selected attributes and methods">
      <add-to-group group-id="IntroduceActionsGroup" anchor="after" relative-to-action="edument.rakuidea.actions.ExtractParentClassAction"/>
    </action>
  </actions>

  <module-components>
  </module-components>

  <project-components>
  </project-components>

  <applicationListeners>
    <listener class="edument.rakuidea.sdk.RakuSdkCacheManager" topic="com.intellij.openapi.project.ProjectManagerListener"/>
  </applicationListeners>

</idea-plugin>
